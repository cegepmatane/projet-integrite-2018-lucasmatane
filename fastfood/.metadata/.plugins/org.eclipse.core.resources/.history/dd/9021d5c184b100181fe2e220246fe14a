package burger;

public class Burger {

	protected String nom;
	protected String fromage;
	protected String prix;
	protected String viande;
	
	public Burger(String nom) {
		super();
		this.nom = nom;
	}
	public Burger(String nom, String fromage) {
		super();
		this.nom = nom;
		this.fromage = fromage;
	}
	public Burger(String nom, String fromage, String prix) {
		super();
		this.nom = nom;
		this.fromage = fromage;
		this.prix = prix;
	}
	public Burger(String nom, String fromage, String prix, String viande) {
		super();
		this.nom = nom;
		this.fromage = fromage;
		this.prix = prix;
		this.viande = viande;
	}
	public String getNom() {
		return nom;
	}
	public void setNom(String nom) {
		this.nom = nom;
	}
	public String getFromage() {
		return fromage;
	}
	public void setFromage(String fromage) {
		this.fromage = fromage;
	}
	public String getViande() {
		return viande;
	}
	public void setViande(String viande) {
		this.viande = viande;
	}
	public String getPrix() {
		return prix;
	}
	public void setPrix(String prix) {
		this.prix = prix;
	}
	
}